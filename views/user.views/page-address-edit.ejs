<body>
  
            <%- include ('./partials/user.header.ejs') %>

    
    <main class="main">
        <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="index-3.html" rel="nofollow">Home</a>
                    <span></span> Pages
                    <span></span> Account
                </div>
            </div>
        </div>
        <section class="pt-150 pb-150">
            <div class="container">
                <div class="row">
                    <div class="col-lg-10 m-auto">
                        <div class="row">
                            
                            <div class="col-md-9">
                                <div class="tab-content test.html">
                                    
                                    <div class="" id="account-detail" role="tabpanel"
                                        aria-labelledby="account-detail-tab">
                                        <div class="card">
                                            <div class="card-header">
                                                <h2>Add a new address</h2>
                                            </div>
                                            <div class="card-body">
                                               
                                                <form method="post" name="enq" action="/user/address/add">
                                                    <div class="row">
                                                        <div class="form-group col-md-12">
                                                            <label>First Name <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="name"
                                                                type="text">
                                                        </div>
                                                        
                                                        
                                                        
                                                        <div class="form-group col-md-12">
                                                            <label>Mobile number <span
                                                                    class="required">*</span></label>
                                                            <input required="" class="form-control square"
                                                                name="mobileNumber" type="text">
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>Pincode <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="pincode" type="text">
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>Flat, House no, Building, Company  <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="houseName" type="text">
                                                        </div><div class="form-group col-md-12">
                                                            <label>Area, Street, Sector, Village <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="area" type="text">
                                                        </div>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>Landmark <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="landmark" type="text">
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>Town <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="town" type="text">
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>State <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="state" type="text">
                                                        </div>
                                                       

                                                        
                                                        <div class="col-md-12">
                                                            <button type="submit" class="btn btn-fill-out submit"
                                                               >Add address</button>
                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>
  
