                    <button class="btn btn-icon btn-aside-minimize"> <i
                            class="text-muted material-icons md-menu_open"></i>
                    </button>
                </div>
            </div>
            <nav>
                <ul class="menu-aside">
                    <li class="menu-item" id="home">
                        <a class="menu-link " > 
                            <span class="text"> </span>
                        </a>
                    </li>
                    <li class="menu-item" id="users">
                        <a class="menu-link" > 
                            <span class="text"></span>
                        </a>
                    </li>

                    <li class="menu-item" id="products">
                        <a class="menu-link"> 
                            <span class="text"></span>
